{
    "name": "charlesportwoodii/ciims",
    "description": "A fast, simple, and easy to use blogging platform and content management system",
    "license": "MIT",
    "type": "project",
    "keywords": [
        "ciims",
        "CiiMS",
        "content management system",
        "blogging platform",
        "redis",
        "mysql"
    ],
    "authors": [
        {
            "name": "Charles R. Portwood II",
            "email": "charlesportwoodii@ethreal.net",
            "homepage": "https://www.erianna.com",
            "role": "Lead Developer"
        }
    ],
    "support": {
        "email": "charlesportwoodii@ethreal.net",
        "issues": "https://github.com/charlesportwoodii/CiiMS/issues",
        "wiki": "https://github.com/charlesportwoodii/CiiMS/wiki/_pages",
        "source": "https://github.com/charlesportwoodii/CiiMS"
    },
    "homepage": "https://www.ciims.org",
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/ircmaxell/PHP-CryptLib"
        },
        {
            "type": "package",
            "package": {
                "name": "gtcode/yii-newrelic",
                "version": "dev-master",
                "source": {
                    "url": "https://github.com/gtcode/yii-newrelic",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "yiqing-95/YiiTimeAgo",
                "version": "dev-master",
                "source": {
                    "url": "https://github.com/yiqing-95/YiiTimeAgo",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ],
    "require": {
        "php": ">=5.3.10",
        "ext-curl": "*",
        "ext-mcrypt": "*",
        "ext-openssl": "*",
        "ext-Phar": "*",
        "ext-pdo_mysql": "*",
        "ext-PDO": "*",
        "ext-Reflection": "*",
        "ext-zip": "*",
        "lib-curl": "*",
        "lib-pcre": "*",
        "lib-openssl": "*",
        "yiisoft/yii": "1.1.14",
        "composer/installers": "v1.0.13",
        "Codeception/YiiBridge": "dev-master",
        "rackspace/php-opencloud": "v1.9.2",
        "ircmaxell/password-compat": "1.0.3",
        "CryptLib/CryptLib": "dev-master",
        "phpmailer/phpmailer": "v5.2.7",
        "knplabs/packagist-api": "v1.1",
        "gtcode/yii-newrelic": "dev-master",
        "yiqing-95/YiiTimeAgo": "dev-master",
        "charlesportwoodii/eanalytics": "1.1.4",
        "ciims-themes/default": "dev-master",
	   "ciims-modules/dashboard": "dev-3.0.0-dev",
        "ciims-modules/api": "dev-2.0.0-dev",
        "ciims-modules/install": "dev-master",
        "ciims-modules/hybridauth": "2.0.4"
    },
    "require-dev": {
	"codeception/codeception": "2.0.*@dev",
        "codeception/YiiBridge": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "Codeception": "src"
        }
    },
    "extra": {
        "installer-paths": {
            "protected/modules/dashboard/": ["ciims-modules/dashboard"],
            "protected/modules/hybridauth/": ["ciims-modules/hybridauth"],
            "protected/modules/install/": ["ciims-modules/install"],
            "protected/modules/api/": ["ciims-modules/api"]
        }
    }
}
