        "rackspace/php-opencloud": "v1.9.2",
        "ircmaxell/password-compat": "1.0.3",
        "phpmailer/phpmailer": "v5.2.7",
        "mobiledetect/mobiledetectlib": "2.7.9",
	"phpunit/phpunit": "4.0.15",
        "phpunit/phpunit-selenium": "1.3.3",
        "charlesportwoodii/eanalytics": "1.1.2",
	"ciims-themes/default": "3.0.1",
	"ciims-modules/dashboard": "dev-master",
	"ciims-modules/api": "dev-master",
	"ciims-modules/install": "dev-master",
	"ciims-modules/hybridauth": "dev-master"
    },
    "extra" : {
	"installer-paths": {
		"protected/modules/dashboard/": ["ciims-modules/dashboard"],
		"protected/modules/hybridauth/": ["ciims-modules/hybridauth"],
		"protected/modules/install/": ["ciims-modules/install"],
		"protected/modules/api/": ["ciims-modules/api"]
